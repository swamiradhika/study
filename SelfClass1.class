public with sharing class SelfClass1 {
	public String param1;
	public String param2;

	// Default Constructor
	public SelfClass1() {
		this.param1 = '';
		this.param2 = '';
	}

	// Parametrised Constructor
	public SelfClass1(String param1, String param2) {
		this.param1 = param1;
		this.param2 = param2;
	}	
}